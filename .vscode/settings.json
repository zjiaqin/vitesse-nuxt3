{
  "prettier.enable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.associations": {
    "*.css": "postcss"
  },
  "editor.formatOnSave": false,
  "i18n-ally.localesPaths": ["lang"],
  "i18n-ally.pathMatcher": "{locale}.json",
  "i18n-ally.dirStructure": "auto",
  "i18n-ally.extract.keygenStrategy": "random",
  "i18n-ally.editor.preferEditor": false,
  "i18n-ally.keystyle": "flat",
  "i18n-ally.displayLanguage": "zh",
  "i18n-ally.includeSubfolders": false,
  "i18n-ally.autoDetection": false,
  "i18n-ally.annotationMaxLength": 20,
  "i18n-ally.extract.keyMaxLength": 0,
  "i18n-ally.extract.targetPickingStrategy": "none",
  "i18n-ally.extract.parsers.html": {
    "attributes": ["text", "title", "alt", "placeholder", "label", "aria-label"],
    "ignoredTags": ["script", "style"],
    "vBind": true,
    "inlineText": true
  },

  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    ".github": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "README.md": true,
    "netlify.toml": true,
    "LICENSE": true,
    "commitlint.config.cjs": true,
    ".stackblitzrc": true,
    ".npmrc": true,
    ".eslintrc": true,
    ".nuxt": true,
    "uno.config.ts": true,
    ".gitignore": true,
    ".vscode": true,
    "tsconfig.json": true,
    "ts-codegen.config.json": true,
    "pnpm-lock.yaml": true,
    "node_modules":true
  },
}
